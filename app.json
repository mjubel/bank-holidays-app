{
  "expo": {
    "name": "BankHolidayApp",
    "slug": "BankHolidayApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bankholiday.app"
    },
    "android": {
      "package": "com.bankholiday.app"
    },
    "plugins": [
      [
        "expo-calendar",
        {
          "calendarPermission": "Allow BankHolidayApp to add events to your calendar."
        }
      ]
    ]
  }
}